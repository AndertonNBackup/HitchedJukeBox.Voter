<ul id="messages">
    {{#if booleans.isArtistSearch}}
        {{#each items}}
            {{> components/artist}}
        {{/each}}
    {{else if booleans.isAlbumSearch}}
        {{#each items}}
            {{> components/album}}
        {{/each}}
    {{else}}
        {{#each items}}
            {{> components/track}}
        {{/each}}
    {{/if}}
</ul>